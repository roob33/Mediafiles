<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mediafiles">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OzActivity"
            android:label="@string/return_to_oz"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".NilsActivity"
            android:label="@string/nils"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity android:name=".Princess"
            android:label="@string/a_little_princess"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Beauty"
            android:label="@string/beauty_and_the_beast"
            android:parentActivityName=".MainActivity">
        </activity>
        

    </application>

</manifest>